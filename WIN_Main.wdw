        code : |1+
         // we must connect by socket
         
         
         // Start the server => To accept the incoming connections
         // Start the server by specifying that the procedure that must be called when receiving a message is the "_SocketMessageReceived" procedure
         // Leave the default port (5023)
         //1: Server
         //2: Client
         SWITCH Dialog(<§?1423C76A00F142A5§>)
         	// Server
         	CASE 1
         		ServerStart(NAME_COMMUNICATION_MESSAGE_SERVER,"pMessageReceived._SocketMessageReceived")
         		
         		// Assign an application name
         		gsApplicationName = "WDCommunication - (ID: " + TimeSys()+")"
         		
         		// We are connected (or the server was started, it will have to be closed)
         		gbSocketConnection = True
         		
         		MyWindow..Plane = PLANE_WAIT_CONNECTION
         		
         		gbServerMode = True
         		
         		
         	// Customer
         	CASE 2
         		
         		// Assign an application name
         		gsApplicationName = "WDCommunication - (ID: " + TimeSys() +")"
         		
         		
         		// Switch to the plane for connecting to the client
         		MyWindow..Plane = PLAN_SOCKET_CLIENT_CONNECTION
         		
         		gbServerMode = False
         		
         		
         END
         
         // make the Home button visible
         BTN_Home..Visible = True
