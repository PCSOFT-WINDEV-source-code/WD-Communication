   -
     name : Disconnection
     procedure_id : 1646133193605500749
     type_code : 14
     code : |1+
      // Summary: <specify the action of the procedure>
      // Syntax:
      // Disconnection ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      PROCEDURE Disconnection()
      
      
      IF gbSocketConnection = True THEN
      	
      	// We are connected to a remote application
      	ClientDisconnection(NAME_COMMUNICATION_MESSAGE)
      	
      	// Stopping the server
      	ServerStop(NAME_COMMUNICATION_MESSAGE_SERVER)
      	
      	// We are not connected anymore
      	gbSocketConnection = False
      	
      	
      	
      END
      
      IF gbMessageConnection = True THEN
      	// we are connected to a local application
      	LocalCancelMessage(NAME_COMMUNICATION_MESSAGE)
      	
      	// We are not connected anymore
      	gbMessageConnection = False
      	
      END
      
      // Clears the list of outgoing messages
      ListDeleteAll(LIST_MESSAGES)
