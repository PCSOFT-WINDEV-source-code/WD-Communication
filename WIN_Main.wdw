        code : |1-
         SWITCH True
         	CASE gbSocketConnection
         		
         		IF gbServerMode
         			// We are connected in socket mode
         			ClientSendMessageServer(NAME_COMMUNICATION_MESSAGE_SERVER,gsApplicationName + TAB + EDT_MESSAGE,True)
         		ELSE
         			// We are connected in socket mode
         			ClientSendMessageServer(NAME_COMMUNICATION_MESSAGE,gsApplicationName + TAB + EDT_MESSAGE,True)	
         		END
         	CASE gbMessageConnection
         		
         		// We are connected in Message mode
         		LocalSendMessage(NAME_COMMUNICATION_MESSAGE,gsApplicationName + TAB + EDT_MESSAGE)
         		
         		
         	OTHER CASE
         		dbgAssert(False,"No connection was found, we should not be positioned on this plane...")
         END
         
         
         ListAdd(LIST_MESSAGES,StringBuild("Message sent: %1",EDT_MESSAGE))
         EDT_MESSAGE = ""
        type : 18
